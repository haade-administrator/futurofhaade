{
  "name": "haade.fr",
  "private": true,
  "version": "1.0.5",
  "description": "official theme of Haade.fr",
  "repository": {
    "type": "git",
    "url": "git://github.com/haade-administrator/haade.fr.git"
  },
  "keywords": [
    "jekyll"
  ],
  "author": "Nicolas Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haade-administrator/haade.fr/issues"
  },
  "homepage": "https://haade-administrator.github.io/haade.fr",
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "cheerio": ">=1.0.0-rc.12",
    "faker": "^5.5.3",
    "gulp-gzip": "^1.4.2",
    "gulp-uglify": "^3.0.2",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "puppeteer": ">=17.1.3"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/jquery-3.6.3.slim.min.js assets/js/flag.js assets/js/prism.js assets/js/clickright.js assets/js/lazyload.js assets/js/btt.js assets/js/lazyload.js assets/js/vanilla-back-to-top.min.js assets/js/navbar.js assets/js/theme.js -c -m -o assets/js/main.min.js",
    "add-banner": "node banner.js",
    "scrap": "node ./_data/aliexpress/fr_FR/launch.js && node ./_data/aliexpress/en_GB/launch.js && node ./_data/domadoo/launch.js && node ./_data/domadoo/launch.js",
    "scrapdom": "node ./_data/domadoo/launch.js",
    "scrapitead": "node ./_data/itead/launch.js",
    "watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
    "build:js": "npm run uglify && npm run add-banner"
  },
  "dependencies": {
    "url": "^0.11.0"
  }
}
